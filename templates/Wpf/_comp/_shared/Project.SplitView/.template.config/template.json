{
    "$schema": "http://json.schemastore.org/template",
    "author": "Microsoft",
    "classifications": [
      "Universal"
    ],
    "name": "wts.Wpf.Project",
    "shortName": "wts.Wpf.Project",
    "identity": "wts.Wpf.Project",
    "tags": {
      "language": "C#",
      "type": "item",
      "wts.type": "composition",
      "wts.platform" : "Wpf",
      "wts.version": "1.0.0",
      "wts.compositionOrder": "0",
      "wts.compositionFilter": "$projectType == SplitView & wts.type == project"
    },
    "sourceName": "wts.ItemName",
    "preferNameDirectory": true,
    "PrimaryOutputs": [
    ],
    "symbols": {
      "wts.rootNamespace": {
        "type": "parameter",
        "replaces": "Param_RootNamespace"
      },
      "wts.homePageName": {
        "type":"parameter",
        "replaces":"Param_HomeName"
      },
      "baseclass": {
        "type": "parameter",
        "replaces": "System.ComponentModel.INotifyPropertyChanged"
      },
      "setter": {
        "type": "parameter",
        "replaces": "Param_Setter"
      },
      "commandclass": {
        "type": "parameter",
        "replaces": "System.Windows.Input.ICommand"
      },
      "canExecute": {
        "type": "parameter",
        "replaces": "CanExecuteChangedMethodName"
      }
    }
  }
