{
  "author": "Laurent Bugnion + Community Contribution",
  "classifications": [
    "Universal"
  ],
  "name": "MVVMLight.Project.VB",
  "tags": {
    "language": "VisualBasic",
    "type": "item",
    "wts.type": "composition",
    "wts.platform": "Uwp",
    "wts.version": "1.0.0",
    "wts.licenses": "[MVVMLight](http://www.galasoft.ch/license_MIT.txt)",
    "wts.compositionFilter": "$framework == MVVMLight & wts.type == project",
    "wts.compositionOrder": "0"
  },
  "sourceName": "wts.ItemName",
  "preferNameDirectory": true,
  "PrimaryOutputs": [
    {
      "path": ".\\ViewModels\\ViewModelLocator.vb"
    },
    {
      "path": ".\\Services\\NavigationServiceEx.vb"
    }
  ],
  "symbols": {
    "wts.projectName": {
      "type": "parameter",
      "replaces": "Param_ProjectName",
      "fileRename": "Param_ProjectName"
    },
    "wts.rootNamespace": {
      "type": "parameter",
      "replaces": "Param_RootNamespace"
    },
    "wts.itemNamespace": {
      "type": "parameter",
      "replaces": "Param_ItemNamespace"
    }
  },
  "postActions": [
    {
      "description": "Add nuget package",
      "manualInstructions": [ ],
      "actionId": "0B814718-16A3-4F7F-89F1-69C0F9170EAD",
      "args": {
        "packageId" : "MvvmLight",
        "version" : "5.4.1.1",
        "projectPath": "Param_ProjectName.vbproj"
      },
      "continueOnError": "true"
    }
  ]
}
